<!-- String-en-US.xaml -->
<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <!-- (1) Title message (MSG_TITLE_...) -->
    <system:String x:Key="MSG_TITLE_TESTE">Title Sample</system:String>

    <!-- (2) Messages (MSG_...) -->
    <system:String x:Key="MSG_SAMPLE1">This is a sample.</system:String>
    <system:String x:Key="MSG_SAMPLE2">This is a sample2.</system:String>

    <!-- (3) Properties -->
    <system:String x:Key="Group1_Prop1">Sample Group Sample(1)</system:String>
    <system:String x:Key="Group1_Prop2">Sample Group Sample(2)</system:String>
    <system:String x:Key="Group1_Prop2_NormalActivitySample">Example Group Example(2)</system:String>

    <!-- (4) Property descriptions -->
    <system:String x:Key="DESC_Group1_Prop1">Sample property1</system:String>
    <system:String x:Key="DESC_Group1_Prop2">Sample property2 for NormalActivitySample</system:String>

</ResourceDictionary>
